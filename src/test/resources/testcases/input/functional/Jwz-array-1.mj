int main() {
    int[] arr = {1, 2, 3};
    println(arr);
    arr[1] = 4;
    println(arr);
    int[][] arr2 = {null, null};
    arr2[1] = new int[3];
    arr2[0] = new int[] {1, 2, 3, 4};
    println(arr2);
    int[] arr3 = {1, 2, 3};
    int[] arr4 = arr3;
    println(arr3 == arr4);
    println(arr3 != arr4);
    arr3[0] = 2;
    println(arr4); // {2, 2, 3}
    return 0;
}